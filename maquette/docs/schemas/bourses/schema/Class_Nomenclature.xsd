<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:include schemaLocation="Simple_Type.xsd"/>
	<xs:complexType name="TLigneNomenclature">
		<xs:sequence>
			<xs:element name="Libelle" type="Base_Texte100"  form="qualified"/>
			<xs:element name="Code" type="Base_Texte24"  form="qualified"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TNomenclature">
		<xs:sequence>
			<xs:element name="Description" type="Base_Texte100"  form="qualified">
				<xs:annotation>
					<xs:documentation>Description du rôle de la nomenclature ou de ses spécificités</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Correspondance" type="TLigneNomenclature" maxOccurs="unbounded"  form="qualified"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
